{
  "env": {
    "browser": true,
    "commonjs": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "extends": ["eslint:recommended", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"]
  }
}
